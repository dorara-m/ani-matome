extends /_include/_layout
append variables
  - var title ='ネタまとめ'
  - var directory ='/neta/'
  - var imgPath = '/assets/img/'
block css
block js

block body
  main.p-under 
    .l-container
      h1 ネタまとめ

      include /_include/_menu
      +PcMenu(1)

      .tableWrap
        table
          thead
            tr
              th サイト
              th サンプル
          tbody
            each item in cmsLinks
              tr
                th
                  a(href=item.link, target="_blank", rel="noopener noreferrer") #{item.title}
                td
                  if item.sample
                    a(href=item.sample) あり→
                  else
                    span -
